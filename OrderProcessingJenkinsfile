pipeline {
	agent none    
stages {
	stage('Docker Network') {
		agent any
		
	}

        stage('Zookeeper') {
			agent any
			steps {
                           waitUntil {
                              sh 'netstat -lntu | grep 3306'
                              echo 'ND'
                           }
                           echo 'SIIIIUUU'
			}
	}
  }
}

