pipeline {
	agent none    
stages {
	stage('Docker Network') {
		agent any
		steps {
			echo 'holaa'	
		}
	}

        stage('Zookeeper') {
			agent any
			steps {
                           waitUntil {
                              sh 'nc -z localhost 3306'
                              echo 'ND'
                           }
                           echo 'SIIIIUUU'
			}
	}
  }
}

