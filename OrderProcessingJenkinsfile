pipeline {
	agent none    
stages {
	stage('Docker Network') {
		agent any
		steps {
			echo 'holaa'	
		}
	}

        stage('Zookeeper') {
			agent any
			when{
				expression {
					return 1 == 1
				}
			}
			steps {
			   comando = $(nc -z localhost 3306)
			   
                           echo 'SIIIIUUU'
			}
	}
  }
}

