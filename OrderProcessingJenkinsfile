pipeline {
	agent none    
stages {
	stage('Docker Network') {
		agent any
		steps {
			echo 'holaa'	
		}
	}

        stage('Zookeeper') {
			agent any
			when{
				expression {
					return ! nc -z localhost 3306
				}
			}
			steps {
                           echo 'SIIIIUUU'
			}
	}
  }
}

